{
    "name": "vue-clean-architecture",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:coverage:watch": "vitest --coverage",
        "test:coverage:open": "vitest run --coverage && powershell -Command \"Invoke-Item coverage/index.html\"",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "format": "biome format",
        "format:fix": "biome format --write",
        "lint": "biome lint",
        "lint:fix": "biome lint --write",
        "check": "biome check",
        "check:fix": "biome check --write && pnpm run type-check",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "deploy-storybook": "bun run build-storybook && gh-pages -d storybook-static"
    },
    "dependencies": {
        "@animxyz/vue3": "^0.6.7",
        "@tailwindcss/vite": "^4.1.11",
        "axios": "^1.11.0",
        "lucide-vue-next": "^0.542.0",
        "pinia": "^3.0.3",
        "tailwindcss": "^4.1.11",
        "vue": "^3.5.17",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@biomejs/biome": "2.1.3",
        "@storybook/addon-docs": "9.1.4",
        "@storybook/vue3-vite": "9.1.4",
        "@tsconfig/node22": "^22.0.2",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.15.32",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vitest/coverage-v8": "3.2.4",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.7.0",
        "gh-pages": "^6.2.0",
        "jiti": "^2.4.2",
        "jsdom": "^26.1.0",
        "npm-run-all2": "^8.0.4",
        "rolldown-vite": "^7.1.0",
        "storybook": "9.1.4",
        "typescript": "~5.8.0",
        "vite": "npm:rolldown-vite@latest",
        "vitest": "^3.2.4",
        "vue-tsc": "^2.2.10"
    }
}
